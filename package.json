{
  "name": "ekaloka-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/frontend",
    "apps/backend",
    "packages/shared",
    "packages/ui"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=apps/frontend\" \"npm run dev --workspace=apps/backend\"",
    "build": "npm run build --workspace=apps/frontend",
    "start": "npm run start --workspace=apps/frontend"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
